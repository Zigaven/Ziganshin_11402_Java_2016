        <constructor-arg ref="comedy"/>
    </bean>
    <bean id="film2" class="Task006.classes.Film">
        <constructor-arg value="Scream"/>
        <constructor-arg ref="imax"/>
        <constructor-arg ref="horror"/>
    </bean>


    <bean id="dream" class="Task006.classes.DreamCinema">
        <constructor-arg value="Dream"/>
        <constructor-arg value="Kazan"/>
    </bean>
    <bean id="karo" class="Task006.classes.KaroFilm">
        <constructor-arg value="Karo"/>
        <constructor-arg value="Kazan"/>
    </bean>
    <bean id="imax" class="Task006.classes.Imax">
        <constructor-arg value="Imax"/>
        <constructor-arg value="Kazan"/>
    </bean>


    <bean id="comedy" class="Task006.classes.Comedy">
        <constructor-arg value="Comedy"/>
    </bean>
    <bean id="fantasy" class="Task006.classes.Fantasy">
        <constructor-arg value="Fantasy"/>
    </bean>
    <bean id="horror" class="Task006.classes.Horror">
        <constructor-arg value="Horror"/>
    </bean>

    <bean id="seller" class="Task006.classes.Seller">
        <constructor-arg value="Seller"/>
        <constructor-arg ref="viewer"/>
    </bean>
    <bean id="viewer" class="Task006.classes.Viewer">
        <constructor-arg value="Viewer"/>
        <constructor-arg ref="imax"/>
    </bean>
