    </bean>
    <bean id="karo1" class="Task010.classes.KaroFilm">
        <constructor-arg value="Karo"/>
        <constructor-arg value="Kazan"/>
    </bean>
    <bean id="imax1" class="Task010.classes.Imax">
        <constructor-arg value="Imax"/>
        <constructor-arg value="Kazan"/>
    </bean>


    <bean id="comedy1" class="Task010.classes.Comedy">
        <constructor-arg value="Comedy"/>
    </bean>
    <bean id="fantasy1" class="Task010.classes.Fantasy">
        <constructor-arg value="Fantasy"/>
    </bean>
    <bean id="horror1" class="Task010.classes.Horror">
        <constructor-arg value="Horror"/>
    </bean>

    <bean id="seller1" class="Task010.classes.Seller">
        <constructor-arg value="Seller"/>
        <constructor-arg ref="viewer1"/>
    </bean>
    <bean id="viewer1" class="Task010.classes.Viewer">
        <constructor-arg value="Viewer"/>
        <constructor-arg ref="imax1"/>
    </bean>
