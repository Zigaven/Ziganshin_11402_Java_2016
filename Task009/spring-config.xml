    <bean id="understudy" class="Task006.classes.Actor">
        <constructor-arg value="Understudy"/>
        <constructor-arg ref="film1"/>
    </bean>
    <bean id="film" class="Task006.classes.Film">
        <constructor-arg value="Avatar"/>
        <constructor-arg ref="karo"/>
        <constructor-arg ref="fantasy"/>
    </bean>
    <bean id="film1" class="Task006.classes.Film">
        <constructor-arg value="Hangover"/>
