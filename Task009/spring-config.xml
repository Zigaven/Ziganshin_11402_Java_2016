    </bean>
    <bean id="film01" class="Task010.classes.Film">
        <constructor-arg value="Hangover"/>
        <constructor-arg ref="dream1"/>
        <constructor-arg ref="comedy1"/>
    </bean>
    <bean id="film02" class="Task010.classes.Film">
        <constructor-arg value="Scream"/>
        <constructor-arg ref="imax1"/>
        <constructor-arg ref="horror1"/>
    </bean>


    <bean id="dream1" class="Task010.classes.DreamCinema">
        <constructor-arg value="Dream"/>
