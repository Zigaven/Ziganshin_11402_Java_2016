    <!--Task010-->
    <bean id="actor1" class="Task010.classes.Actor">
        <constructor-arg value="actor"/>
        <constructor-arg ref="film0"/>
    </bean>
    <bean id="stuntman1" class="Task010.classes.StuntmanActor">
        <constructor-arg value="Stuntman"/>
